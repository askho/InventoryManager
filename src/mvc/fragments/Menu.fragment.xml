<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:data="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<ResponsivePopover
		title="Menu"
		contentWidth="15rem"
		placement="Bottom">
		<content>
			<List
				class="appList">
				<StandardListItem
					title="Events"
					type="Active"
					data:navView="Events"
					press="onMenuItemPress"
					iconDensityAware="false"
					iconInset="false" />
					<StandardListItem
					title="Inventory"
					type="Active"
					data:navView="Inventory"
					press="onMenuItemPress"
					iconDensityAware="false"
					iconInset="false" />
					<StandardListItem
					title="Members"
					type="Active"
					data:navView="Members"
					press="onMenuItemPress"
					iconDensityAware="false"
					iconInset="false" />
			</List>
		</content>
	</ResponsivePopover>
</core:FragmentDefinition>