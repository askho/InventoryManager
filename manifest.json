{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "com.scout138.inventoryManager",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"ach": "CA-UI5-DOC"
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": "com.scout138.inventoryManager.mvc.view.App",
		"dependencies": {
			"minUI5Version": "1.30",
			"libs": {
				"sap.m": {}
			}
		},
		"config": {
		  	"oDataServiceMock": "http://localhost/InventoryManager/mock/events.json",
		  	"oDataService": "/remote/"
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.scout138.inventoryManager.i18n.i18n"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.scout138.inventoryManager.mvc.view",
				"controlId": "idAppControl",
				"controlAggregation": "masterPages"
			},
			"routes": [{
				"pattern": "",
				"name": "Master",
				"target": "Master"
			}, {
				"pattern": "Detail",
				"name": "Detail",
				"target": "Detail"
			}],
			"targets": {
				"Master": {
					"viewName": "Master"
				},
				"Detail": {
					"viewName": "Detail"
				}
			}
		}
	}
}