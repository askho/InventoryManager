<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://odata.inventory.leonho.ca:80/</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>http://leonho.cloudapp.net/</MSDeployServiceURL>
    <DeployIisAppPath>odata.Inventory</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>RemoteAgent</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>LEONHO\askho</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="InventoryManagerDatabaseEntities" Order="1" Enabled="False">
          <Destination Path="Data Source=localhost;Initial Catalog=InventoryManagerDatabase;User ID=inventoryManagerDeployment;Password=password123" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="oDataService.Models.InventoryManagerDatabaseEntities, oDataService" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
    <_DestinationType>AzureVirtualMachine</_DestinationType>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)InventoryManagerDatabaseEntities-Web.config Connection String">
      <ParameterValue>metadata=res://*/Models.InventoryManager.csdl|res://*/Models.InventoryManager.ssdl|res://*/Models.InventoryManager.msl;provider=System.Data.SqlClient;provider connection string="Data Source=localhost;Initial Catalog=InventoryManagerDatabase;User ID=inventoryManagerDeployment;Password=password123"</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>