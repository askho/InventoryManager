<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ResponsivePopover
		title="Menu"
		contentWidth="15rem"
		placement="Bottom">
		<content>
			<List
				id="list"
				class="appList">
				<StandardListItem
					title="Events"
					type="Active"
					press="onNavToProduct"
					iconDensityAware="false"
					iconInset="false" />
					<StandardListItem
					title="Inventory"
					type="Active"
					press="onNavToProduct"
					iconDensityAware="false"
					iconInset="false" />
					<StandardListItem
					title="Members"
					type="Active"
					press="onNavToProduct"
					iconDensityAware="false"
					iconInset="false" />
			</List>
		</content>
	</ResponsivePopover>
</core:FragmentDefinition>