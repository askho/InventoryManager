{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "com.scout138.inventoryManager",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"ach": "CA-UI5-DOC"
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": "com.scout138.inventoryManager.mvc.view.App",
		"dependencies": {
			"minUI5Version": "1.30",
			"libs": {
				"sap.m": {}
			}
		},
		"config": {
		  	"oDataServiceMock": "http://localhost/InventoryManager/mock/events.json",
		  	"oDataService": "/remote/",
		  	"state": {
		  		"auth": {
		  			"loggedin": false
		  		}
		  	}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.scout138.inventoryManager.i18n.i18n"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.scout138.inventoryManager.mvc.view",
				"controlAggregation": "masterPages"
			},
			"routes": [
				{
					"pattern": "",
					"name": "Master",
					"view": "Master",
					"targetAggregation" : "masterPages",
					"targetControl" : "idAppControl",
					"subroutes" : [
						{
							"pattern": "Detail/{detailID}",
							"name":"Detail",
							"view":"Detail",
							"targetAggregation": "detailPages"
						}
					]
				}, {
					"pattern": "Inventory",
					"name": "Inventory",
					"view": "Inventory",
					"targetAggregation": "masterPages",
					"targetControl": "idAppControl",
					"subroutes" : [
						{
							"pattern": "InventoryDetail/{detailID}/{itemName}/{itemDescription}",
							"name": "InventoryDetail",
							"view": "InventoryDetail",
							"targetAggregation": "detailPages"
						}
					]
				}, {
					"pattern": "Members",
					"name": "Members",
					"view": "Members",
					"targetAggregation": "masterPages",
					"targetControl": "idAppControl",
					"subroutes" : [
						{
							"pattern": "MembersDetail/{detailID}/{fName}/{lName}",
							"name": "MembersDetail",
							"view": "MembersDetail",
							"targetAggregation": "detailPages"
						}
					]
				}
			]
		}
	}
}